<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.StationManager.app.storage.database.dto.HallsTicketOffices" table="halls_ticket_offices">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="increment"/>
        </id>
        <many-to-one name="hall" class="com.StationManager.app.storage.database.dto.HallDTO" column="hall_id"/>
        <many-to-one name="ticketOffice" class="com.StationManager.app.storage.database.dto.TicketOfficeDTO"
                     column="ticket_office_id"/>
    </class>
</hibernate-mapping>

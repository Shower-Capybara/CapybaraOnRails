<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.StationManager.app.storage.database.dto">
    <class name="ClientPositions" table="client_positions">
        <id name="id" type="java.lang.Integer">
            <column name="id"/>
            <generator class="identity"/>
        </id>

        <many-to-one name="client" class="com.StationManager.app.domain.client.Client" column="client_id"/>

        <many-to-one name="ticketOffice" class="com.StationManager.app.storage.database.dto.TicketOfficeDTO"
                     column="ticket_office_id"/>

        <property name="positionInQueue" type="java.lang.Integer">
            <column name="position_in_queue"/>
        </property>
    </class>
</hibernate-mapping>

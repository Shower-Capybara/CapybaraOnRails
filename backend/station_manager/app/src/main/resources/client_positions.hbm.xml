<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.StationManager.app.storage.database.dto">
    <class name="ClientPositions" table="client_positions">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <!-- Reference to the Client entity -->
        <many-to-one name="client" column="client_id" class="com.StationManager.app.domain.client.Client" update="false" />
        <!-- Reference to the TicketOffice entity -->
        <many-to-one name="ticketOffice" column="ticket_office_id" class="com.StationManager.app.domain.train_station.TicketOffice" not-null="true" />
<!--        <property name="ticketOfficeId" column="ticket_office_id" type="integer" />-->
        <property name="positionInQueue" column="position_in_queue" update="false" />
    </class>
</hibernate-mapping>
